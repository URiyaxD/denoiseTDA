## The function is currently defined as
function (init = c(0, 0), R = 1, N = 100) 
{
    fr <- 360/N
    x <- c()
    y <- c()
    ang = 0
    while (ang < 360) {
        x <- c(x + init[1], R * cos(ang))
        y <- c(y + init[2], R * sin(ang))
        ang <- ang + fr
    }
    return(matrix(c(x, y), nrow = length(x), ncol = 2))
  }
}
